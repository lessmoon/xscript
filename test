"jump":"beg"
<"multiple">
"set":"tmp_b" "${b}"
"set":"res" "0"
<"m_loop">
"condition":"${tmp_b}" "0" "end_loop"
"add":"tmp_b" "-1"
"add":"res" "${a}"
"jump":"m_loop"
<"end_loop">
"jump":"${back}"



